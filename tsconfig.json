{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strictNullChecks": true,
    "noImplicitAny": false,
		"resolveJsonModule": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "isolatedModules": true
  },
  "include": [
    "./src/**/*.ts"
  ]
}
