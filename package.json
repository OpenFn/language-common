{
  "name": "@openfn/language-common",
  "version": "2.0.0-pre1",
  "description": "Common Expressions for OpenFn",
  "scripts": {
    "build": "node esbuild.js",
    "gen:types": "tsc -p tsconfig.bundle.json",
    "gen:dts-rollup": "api-extractor run --local --diagnostic",
    "build:all": "yarn run clean && yarn run gen:types && yarn run gen:dts-rollup && yarn run build",
    "clean": "rimraf dist/ types/",
    "test": "mocha --require ts-node/register test/*.ts",
    "test:watch": "mocha -w --require ts-node/register test/*.ts",
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/language-common.d.ts"
      },
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jsonpath-plus": "^4.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.26.0",
    "@openfn/simple-ast": "^0.3.2",
    "@types/mocha": "^8.2.3",
    "@types/node": "^18.0.0",
    "chai": "^4.2.0",
    "deep-eql": "^4.0.0",
    "esbuild": "^0.14.47",
    "mocha": "^7.1.0",
    "nock": "^13.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.7.4"
  },
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenFn/language-common.git"
  },
  "bugs": {
    "url": "https://github.com/OpenFn/language-common/issues"
  },
  "homepage": "https://github.com/OpenFn/language-common#readme",
  "bundledDependencies": [
    "axios",
    "jsonpath-plus"
  ],
  "license": "LGPL-3.0-or-later"
}
